{
  "c": {
    "type": "compiler",
    "compile": [ "/usr/bin/gcc", "-w", "--std=c99", "-o", "foo", "foo.c", "-lm", "-I/include" ],
    "codeFile": "foo.c",
    "outputFile": "foo",
    "execute": ["foo"]
  },
  "cc": {
    "type": "compiler",
    "compile": [ "/usr/bin/g++", "-O2", "-w", "-std=c++17", "-o", "foo", "foo.cc", "-lm", "-I/include" ],
    "codeFile": "foo.cc",
    "outputFile": "foo",
    "execute": ["foo"]
  },
  "java": {
    "type": "compiler",
    "compile": ["javac", "-encoding", "UTF-8", "Main.java"],
    "codeFile": "Main.java",
    "outputFile": "Main.class",
    "execute": ["java", "-Dfile.encoding=UTF-8", "Main"]
  },
  "py3": {
    "type": "compiler",
    "compile": ["python3", "-c", "import py_compile; py_compile.compile('foo.py', 'foo.pyc', doraise=True)"],
    "codeFile": "foo.py",
    "outputFile": "foo.pyc",
    "execute": ["python3", "foo.pyc"]
  },
  "js": {
    "type": "interpreter",
    "compile": ["true"],
    "codeFile": "foo.js",
    "outputFile": "foo.js",
    "execute": ["nodejs", "foo.js"]
  },
  "cs": {
    "type": "compiler",
    "compile":  ["mono-csc", "-optimize+", "-out:foo.exe", "foo.cs"],
    "codeFile": "foo.cs",
    "outputFile": "foo.exe",
    "execute": ["mono", "foo.exe"]
  },
  "cs_bflat": {
    "type": "compiler",
    "compile":  ["bflat", "build", "--optimize-time", "--out", "foo.exe", "foo.cs"],
    "codeFile": "foo.cs",
    "outputFile": "foo.exe",
    "execute": ["foo.exe"]
  }
}
